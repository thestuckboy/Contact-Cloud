CREATE DATABASE lista_contactos;
USE lista_contactos;
CREATE TABLE contactos(
    id INT(11) NOT NULL AUTO_INCREMENT,
    names VARCHAR(60) NOT NULL,
    lastname VARCHAR(60) NOT NULL,
    phone VARCHAR(20) NOT NULL,
    PRIMARY KEY (id)
);

